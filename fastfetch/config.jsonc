      {
          "type": "shell",
          "key": "└  ",
          "keyColor": "33", 
      },
      "break",
      "break",
      {
          "type": "wmtheme",
          "key": "󰉼 ",
          "keyColor": "33",
      },
      {
          "type": "icons",
          "key": "󰀻 ",
          "keyColor": "33",
      },
      {
          "type": "cursor",
          "key": " ",
          "keyColor": "33", 
      },
      {
