  "display": {
        "separator": " "
  },
  "logo": {
    "padding": {
      "left": 2,
      "right": 2,
      "top": 2
    },
    "color": {
        "1": "yellow",
        "2": "magenta"
    }
  },

  "modules": [
      "break",
      {
          "type": "kernel",
